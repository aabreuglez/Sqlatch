    "flux-standard-action": "^0.6.1",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "~3.0.2",
    "react-hot-loader": "1.3.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
