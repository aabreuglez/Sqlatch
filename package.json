{
  "name": "react-webpack-babel",
  "version": "0.0.3",
  "description": "React Webpack Babel Starter Kit",
  "main": "''",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "test": "mocha --compilers js:babel-register --recursive",
    "test-ci": "mocha --compilers js:babel-register --recursive -R xunit test/ > test-reports.xml",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alicoding/react-webpack-babel"
  },
  "author": "Ali Al Dallal",
  "license": "MIT",
  "homepage": "https://github.com/alicoding/react-webpack-babel#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "~4.15.0",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "~4.4.5",
    "redux": "~3.5.2",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "~6.14.0",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
